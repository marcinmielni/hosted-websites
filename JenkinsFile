                sh "cd marcin.mielnicz.uk"
                sh "ls"
                sh "hugo config"
                sh "hugo"
            }
        }
        stage("test") {
            steps {
               echo "dummy testing XD"
            }
        }
        stage("deploy") {
            steps {
                sh deploy.sh
            }
        }
    }
}
